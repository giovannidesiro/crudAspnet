@model IEnumerable<crudaspnet.Models.eletronico>

@{
    ViewBag.Title = "Index";
}

<h2>Eletrônicos</h2>

<p>
    @Html.ActionLink("Cadastrar", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.aparelho)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descricao)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.aparelho)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descricao)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.codigo }) |
            @Html.ActionLink("Detalhes", "Details", new { id=item.codigo }) |
            @Html.ActionLink("Deletar", "Delete", new { id=item.codigo })
        </td>
    </tr>
}

</table>
